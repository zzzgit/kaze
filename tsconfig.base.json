{
	"compilerOptions": {
		"target": "ES2017",
		"lib": ["esnext"],
		"module": "commonjs",
		// "sourceMap": false,
		"moduleResolution": "node",
		"strictNullChecks": true,
		"removeComments": true,
		"declaration": true,
		"outDir": "./built/lib/",
		"declarationDir": "./built/t/",
		"emitDeclarationOnly": true,
		"declarationMap": true,
		"preserveWatchOutput": true,
		"alwaysStrict": true,
		"diagnostics": true,
		"strict": true,
		"noImplicitAny": true,
		"noUnusedParameters": true,
		"noUnusedLocals": true,
		"noImplicitThis": true,
		"listFiles": true,
		"pretty": false,
		"noEmitOnError": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitReturns": true,
		"allowSyntheticDefaultImports": true
	},
}
